{
  "name": "@cheatron/native-mock-koffi",
  "version": "1.0.0",
  "description": "Pure JavaScript mock implementation of koffi for testing native modules without compilation",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "LICENSE"
  ],
  "license": "MIT",
  "keywords": [
    "cheatron",
    "mock",
    "koffi",
    "native",
    "windows",
    "testing",
    "typescript"
  ],
  "author": "Cheatron",
  "homepage": "https://github.com/Cheatron/cheatron-native-mock-koffi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cheatron/cheatron-native-mock-koffi.git"
  },
  "bugs": {
    "url": "https://github.com/Cheatron/cheatron-native-mock-koffi/issues"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"{src,tests}/**/*.{js,ts,jsx,tsx}\" --cache",
    "lint:fix": "eslint \"{src,tests}/**/*.{js,ts,jsx,tsx}\" --fix",
    "format": "prettier --write \"{src,tests}/**/*.{js,ts,jsx,tsx,css,md,json}\"",
    "format:check": "prettier --check \"{src,tests}/**/*.{js,ts,jsx,tsx,css,md,json}\""
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/bun": "latest",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
